 ...state,\n        posts: action.payload.data,\n        currentPage: action.payload.currentPage,\n        numberOfPages: action.payload.numberOfPages,\n      };\n    case FETCH_BY_SEARCH:\n      return { ...state, posts: action.payload.data };\n    case FETCH_POST:\n      return { ...state, post: action.payload.post };\n    case LIKE:\n      return { ...state, posts: state.posts.map((post) => (post._id === action.payload._id ? action.payload : post)) };\n    case COMMENT:\n      return {\n        ...state,\n        posts: state.posts.map((post) => {\n          if (post._id === +action.payload._id) {\n            return action.payload;\n          }\n          return post;\n        }),\n      };\n    case CREATE:\n      return { ...state, posts: [...state.posts, action.payload] };\n    case UPDATE:\n      return { ...state, posts: state.posts.map((post) => (post._id === action.payload._id ? action.payload : post)) };\n    case DELETE:\n      return { ...state, posts: state.posts.filter((post) => post._id !== action.payload) };\n    default:\n      return state;\n  }\n};\n"],"mappings":"AAAA,OAASA,SAAS,CAAEC,eAAe,CAAEC,UAAU,CAAEC,MAAM,CAAEC,MAAM,CAAEC,MAAM,CAAEC,IAAI,CAAEC,OAAO,KAAQ,0BAA0B,CAExH,eAAe,UAAoD,IAAnD,CAAAC,KAAK,CAAAC,SAAA,CAAAC,MAAA,IAAAD,SAAA,MAAAE,SAAA,CAAAF,SAAA,IAAG,CAAEG,SAAS,CAAE,IAAI,CAAEC,KAAK,CAAE,EAAG,CAAC,IAAE,CAAAC,MAAM,CAAAL,SAAA,CAAAC,MAAA,GAAAD,SAAA,IAAAE,SAAA,CAC5D,OAAQG,MAAM,CAACC,IAAI,EACjB,IAAK,eAAe,CAClB,MAAO,CAAE,GAAGP,KAAK,CAAEI,SAAS,CAAE,IAAK,CAAC,CACtC,IAAK,aAAa,CAChB,MAAO,CAAE,GAAGJ,KAAK,CAAEI,SAAS,CAAE,KAAM,CAAC,CACvC,IAAK,CAAAZ,SAAS,CACZ,MAAO,CACL,GAAGQ,KAAK,CACRK,KAAK,CAAEC,MAAM,CAACE,OAAO,CAACC,IAAI,CAC1BC,WAAW,CAAEJ,MAAM,CAACE,OAAO,CAACE,WAAW,CACvCC,aAAa,CAAEL,MAAM,CAACE,OAAO,CAACG,aAChC,CAAC,CACH,IAAK,CAAAlB,eAAe,CAClB,MAAO,CAAE,GAAGO,KAAK,CAAEK,KAAK,CAAEC,MAAM,CAACE,OAAO,CAACC,IAAK,CAAC,CACjD,IAAK,CAAAf,UAAU,CACb,MAAO,CAAE,GAAGM,KAAK,CAAEY,IAAI,CAAEN,MAAM,CAACE,OAAO,CAACI,IAAK,CAAC,CAChD,IAAK,CAAAd,IAAI,CACP,MAAO,CAAE,GAAGE,